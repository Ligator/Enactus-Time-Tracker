<table class="table table-striped">
  <thead>
    <tr>
      <th>Apellidos</th>
      <th>Nombre(s)</th>
      <th>Email</th>
      <th>Proyecto</th>
      <th>Posici√≥n</th>
    </tr>
  </thead>
  <tbody>
    <% all_enacters.each do |enacter| %>
      <tr>
        <td><%= enacter.lname %></td>
        <td><%= enacter.fname %></td>
        <td><%= enacter.email %></td>
        <td><%= select_tag "project_id[#{enacter.id}]", options_from_collection_for_select(@projects, "id", "name", enacter.project_id), include_blank: "Ninguno", class: "form-control" %></td>
        <% if enacter.id == current_user.id %>
          <td><%= enacter.position_title %></td>
        <% else %>
          <td><%= select_tag "position[#{enacter.id}]", options_for_select(@positions_array, enacter.position), include_blank: "Ninguno", class: "form-control" %></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>
